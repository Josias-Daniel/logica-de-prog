algoritmo "semnome"
var
   jd,valor,np: real
inicio
      jd <- 0
      Escreval("quer qual valor inicial?")
      Leia(jd)
      Escreval("qual valor do salto entre os números?")
      Leia(valor)
      Escreval("quer contar até quanto?")
      Leia(np)
      Enquanto (jd <= np) faca
               escreval(jd)
             jd <- jd + valor]
      fimenquanto
fimalgoritmo